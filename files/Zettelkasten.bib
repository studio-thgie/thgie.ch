@article{ozkaynakSystematicReviewDesign2021,
  title = {A {{Systematic Review}} of {{Design Workshops}} for {{Health Information Technologies}}},
  author = {Ozkaynak, Mustafa and Sircar, Christina M. and Frye, Olivia and Valdez, Rupa S.},
  date = {2021-05-14},
  journaltitle = {Informatics},
