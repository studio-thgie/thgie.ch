@article{hurleyCommunityArchivesCommunity2016,
  title = {Community {{Archives}}, {{Community Clouds}}: {{Enabling Digital Preservation}} for {{Small Archives}}},
  shorttitle = {Community {{Archives}}, {{Community Clouds}}},
