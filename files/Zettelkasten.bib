  author = {Kbar, Ghassan and Bhatia, Akshay and Abidi, Mustufa Haider and Alsharawy, Ibraheem},
  date = {2017-01-02},
  journaltitle = {Disability and Rehabilitation: Assistive Technology},
  volume = {12},
  number = {1},
  eprint = {26882961},
