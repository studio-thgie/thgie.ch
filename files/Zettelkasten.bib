@article{willisOntologicalDesigning2006c,
  title = {Ontological {{Designing}}},
  author = {Willis, Anne-Marie},
  date = {2006-06-01},
  journaltitle = {Design Philosophy Papers},
  volume = {4},
  number = {2},
  pages = {69--92},
  issn = {null},
  doi = {10.2752/144871306X13966268131514},
  url = {https://doi.org/10.2752/144871306X13966268131514},
  urldate = {2019-12-13},
  keywords = {Read},
  note = {\textbf{Extracted Annotations (12/6/2020, 6:47:29 PM)}
\par
"ontological designing is a way of characterising the relation between human beings and lifeworlds." (\href{zotero://open-pdf/library/items/4E2PW9N7?page=1}{Willis 2006:69})
\par
