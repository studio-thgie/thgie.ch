@incollection{assmannArchiveImWandel2009,
  title = {Archive Im {{Wandel}} Der {{Mediengeschichte}}},
  booktitle = {Archivologie: {{Theorien}} Des {{Archivs}} in {{Wissenschaft}}, {{Medien}} Und {{Künsten}}},
  author = {Assmann, Aleida},
  editor = {Ebeling, Knut and Günzel, Stephan},
  date = {2009},
