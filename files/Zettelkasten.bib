@article{starEthnographyInfrastructure1999,
  title = {The {{Ethnography}} of {{Infrastructure}}},
  author = {Star, Susan Leigh},
  date = {1999-11-01},
  journaltitle = {American Behavioral Scientist},
  volume = {43},
