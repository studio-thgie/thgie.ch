@article{ahmedAnthropomorphizingArtificialIntelligence2020,
  title = {Anthropomorphizing Artificial Intelligence: Towards a User-Centered Approach for Addressing the Challenges of over-Automation and Design Understandability in Smart Homes},
  shorttitle = {Anthropomorphizing Artificial Intelligence},
  author = {Ahmed, Danyal},
  year = {2020},
  month = aug,
  journal = {Intelligent Buildings International},
  pages = {1--14},
  issn = {1750-8975, 1756-6932},
  doi = {10.1080/17508975.2020.1795612},
  abstract = {Smart home, as a comfort and leisure providing equipment/device/object instead of its initial role of to serve, support, monitor and deliver therapy, is facing many challenges towards its successful integration with user's lifestyles. Among the current technical and social challenges \textendash interoperability, reliability, privacy, security, consumer's lack of awareness about its benefits, costs and complexity of technology, maintenance costs, lack of incentive for internet providers etc. \textendash{} take the lead among many others. Limiting the scope, this paper attempts to address challenges of (a) over-automation \textendash{} a feeling regarding advanced technologies of being out-of-control, and (b) difficulty in understandability of equipment's/device's/object's design \textendash{} users must be straight-forwardly able to operate them without any particular operational knowledge. Literature from the disciplines of artificial intelligence (hereinafter AI), product design (industrial engineering), human factors (ergonomics), social psychology, communication and sociology (sociological experiments) have been incorporated in this study to understand the psychology of human thought and cognition, everyday things and designer's ability of utilizing these psychologies and of users for addressing, first, the role that a smart home can play as a leisure equipment/device/object, and secondly, granting its user the control and ease of operability they desire.},
  langid = {english},
  file = {/Users/adriandemleitner/Zotero/storage/U3ZBRXKZ/Ahmed_2020_Anthropomorphizing artificial intelligence.pdf}
}

