@incollection{assmannArchiveImWandel2009,
  title = {Archive Im {{Wandel}} Der {{Mediengeschichte}}},
  booktitle = {Archivologie: {{Theorien}} Des {{Archivs}} in {{Wissenschaft}}, {{Medien}} Und {{K\"unsten}}},
  author = {Assmann, Aleida},
  editor = {Ebeling, Knut and G{\"u}nzel, Stephan},
