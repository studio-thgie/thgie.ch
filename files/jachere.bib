@article{sillarSocialAgencyThings2009,
  title = {The {{Social Agency}} of {{Things}}? {{Animism}} and {{Materiality}} in the {{Andes}}},
  shorttitle = {The {{Social Agency}} of {{Things}}?},
  author = {Sillar, Bill},
  year = {2009},
  month = oct,
  journal = {CAJ},
  volume = {19},
  number = {3},
  pages = {367--377},
  issn = {0959-7743, 1474-0540},
  doi = {10.1017/S0959774309000559},
